{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug interp_etap2",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/interp_etap2",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        { "name": "LD_LIBRARY_PATH", "value": "./libs" }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "preLaunchTask": "Build Etap2 (debug)",
      "setupCommands": [
        { "text": "-enable-pretty-printing", "description": "Enable GDB pretty printing", "ignoreFailures": true }
      ]
    },
    {
      "name": "Debug interp (Etap1)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/interp",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        { "name": "LD_LIBRARY_PATH", "value": "./libs" }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "preLaunchTask": "Build Etap2 (debug)",
      "setupCommands": [
        { "text": "-enable-pretty-printing", "description": "Enable GDB pretty printing", "ignoreFailures": true }
      ]
    }
  ]
}
